@model NHSD.GPIT.BuyingCatalogue.WebApp.Areas.Order.Models.DeleteOrder.DeleteConfirmationModel;
@{
    ViewData["Title"] = Model.Title;
}

<div class="nhsuk-width-container" data-test-id="delete-order-confirmation-page">
    <partial name="Partials/_BackLink" model="Model" />
    <div class="nhsuk-grid-row">
        <div class="nhsuk-grid-column-two-thirds">
            <nhs-page-title title="@Model.Title" advice="This order has been deleted." />
            <h2 data-test-id="order-description-title">Order description</h2>
            <p data-test-id="order-description">@Model.Description</p>
        </div>
    </div>
</div>
