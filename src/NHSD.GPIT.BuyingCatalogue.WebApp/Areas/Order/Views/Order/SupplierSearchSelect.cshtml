@model NHSD.GPIT.BuyingCatalogue.WebApp.Areas.Order.Models.SupplierSearchSelectModel;
@{
    ViewData["Title"] = $"Suppliers found";
}


<div class="nhsuk-width-container" data-test-id="supplier-select-page">
    <div data-test-id="go-back-link">
        <div class="nhsuk-back-link">
            <a class="nhsuk-back-link__link" href="/order/organisation/03F/order/C010004-01/supplier/search">
                <svg class="nhsuk-icon nhsuk-icon__chevron-left" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" aria-hidden="true">
                    <path d="M8.5 12c0-.3.1-.5.3-.7l5-5c.4-.4 1-.4 1.4 0s.4 1 0 1.4L10.9 12l4.3 4.3c.4.4.4 1 0 1.4s-1 .4-1.4 0l-5-5c-.2-.2-.3-.4-.3-.7z"></path>
                </svg>
                Go back
            </a>
        </div>

    </div>

    <div class="nhsuk-grid-row">
        <div class="nhsuk-grid-column-two-thirds">


            <h1 data-test-id="supplier-select-page-title" class="nhsuk-u-margin-top-7 nhsuk-u-margin-bottom-3">Suppliers found</h1>
            <p data-test-id="supplier-select-page-description" class="nhsuk-heading-l nhsuk-u-font-size-24 nhsuk-u-margin-bottom-6">These are the suppliers that match the search terms you provided. Select the one you&#39;re looking for.</p>

            <form method="post">
                <input type="hidden" name="_csrf" value="LdNuLMXV-j7N9v_b_bwyA3xNrds62UT5LS8k" />









                <div id="selectSupplier" data-test-id="question-selectSupplier">

                    <div data-test-id="radiobutton-options">









                        <div class="nhsuk-form-group">

                            <fieldset class="nhsuk-fieldset">
                                <legend class="nhsuk-fieldset__legend nhsuk-u-font-size-24 nhsuk-u-font-weight-bold nhsuk-u-margin-bottom-2">
                                    Select Supplier
                                </legend>

                                <div class="nhsuk-radios">

                                    <div class="nhsuk-radios__item">
                                        <input class="nhsuk-radios__input" id="selectSupplier-1" name="selectSupplier" type="radio" value="10000">
                                        <label class="nhsuk-label nhsuk-radios__label" for="selectSupplier-1">
                                            EMIS Health
                                        </label>
                                    </div>


                                    <div class="nhsuk-radios__item">
                                        <input class="nhsuk-radios__input" id="selectSupplier-2" name="selectSupplier" type="radio" value="99999">
                                        <label class="nhsuk-label nhsuk-radios__label" for="selectSupplier-2">
                                            NotEmis Health
                                        </label>
                                    </div>

                                </div>
                            </fieldset>

                        </div>

                    </div>

                </div>










                <div data-test-id="continue-button">










                    <button class="nhsuk-button nhsuk-u-margin-top-9" type="submit">
                        Continue
                    </button>



                </div>

            </form>
        </div>
    </div>
