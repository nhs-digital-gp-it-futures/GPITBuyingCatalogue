@using NHSD.GPIT.BuyingCatalogue.Framework.Extensions;
@using NHSD.GPIT.BuyingCatalogue.WebApp.Controllers
@model NHSD.GPIT.BuyingCatalogue.WebApp.Models.NavBaseModel
@{
    ViewBag.Title = "Account locked";
}
<partial name="Partials/_BackLink" model="@Model" />
<div class="nhsuk-grid-row">
    <div class="nhsuk-grid-column-two-thirds">
        <nhs-page-title title="@ViewBag.Title" advice="Your account has been temporarily locked after too many failed attempts to log in. You can try to log in again in 5 minutes." />
    
        <vc:nhs-secondary-button text="Return to homepage"
                                 type="Primary"
                                 url="@Url.Action(
                                          nameof(HomeController.Index),
                                          typeof(HomeController).ControllerName())"/>
        </div>
</div>
