@model NHSD.GPIT.BuyingCatalogue.WebApp.Areas.Admin.Models.ImportModels.ImportGpPracticeListModel
@{
    ViewBag.Title = "Import GP Practice List";
}

<div data-test-id="import-gp-practice-list-page">
    <partial name="Partials/_BackLink" model="Model" />
    <div class="nhsuk-grid-row">
        <div class="nhsuk-grid-column-two-thirds">
            <nhs-validation-summary />
            <nhs-page-title title="@ViewBag.Title" />
            <form method="post">
                <nhs-input
                    asp-for="@Model.CsvUrl" 
                    label-text="CSV Url" />
                <div data-test-id="import-gp-practice-list-button">
                    <nhs-submit-button text="Import" />
                </div>
            </form>
        </div>
    </div>
</div>
