@model NHSD.GPIT.BuyingCatalogue.WebApp.Areas.Admin.Models.ImportModels.ImportGpPracticeListConfirmationModel
@{
    ViewBag.Title = "Import GP Practice List";
}

<div data-test-id="import-gp-practice-list-confirmation-page">
    <partial name="Partials/_BackLink" model="Model" />
    <div data-test-id="import-gp-practice-list-confirmation-page-title">
        <nhs-page-title title="@ViewBag.Title"
                        advice="Import started."/>
        <p>The file at @Model.CsvUrl is being imported.</p>
        <p>An email confirmation will be sent to @Model.EmailAddress once the import has completed.</p>
    </div>
</div>
