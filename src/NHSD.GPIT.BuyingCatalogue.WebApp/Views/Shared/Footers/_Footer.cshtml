<footer role="contentinfo">
    <div class="nhsuk-footer-container">
        <div class="nhsuk-width-container">
            <h2 class="nhsuk-u-visually-hidden">Support links</h2>
            <div class="nhsuk-footer">

                <ul class="nhsuk-footer__list">
                    <li class="nhsuk-footer__list-item nhsuk-footer-default__list-item">
                        <a class="nhsuk-footer__list-item-link"
                           asp-action="@nameof(HomeController.AccessibilityStatement)"
                           asp-controller="@typeof(HomeController).ControllerName()">Accessibility statement</a>
                    </li>
                    <li class="nhsuk-footer__list-item nhsuk-footer-default__list-item">
                        <a class="nhsuk-footer__list-item-link" href="https://digital.nhs.uk/services/digital-care-services-catalogue/nhs-digital-buying-catalogue">About us</a>
                    </li>
                    <li class="nhsuk-footer__list-item nhsuk-footer-default__list-item">
                        <a class="nhsuk-footer__list-item-link"
                           asp-action="@nameof(HomeController.ContactUs)"
                           asp-controller="@typeof(HomeController).ControllerName()">Contact us</a>
                    </li>
                    <li class="nhsuk-footer__list-item nhsuk-footer-default__list-item">
                        <a class="nhsuk-footer__list-item-link"
                           asp-action="@nameof(HomeController.PrivacyPolicy)"
                           asp-controller="@typeof(HomeController).ControllerName()">Privacy policy and cookies</a>
                    </li>
                    <li class="nhsuk-footer__list-item nhsuk-footer-default__list-item">
                        <a class="nhsuk-footer__list-item-link"
                           asp-action="@nameof(TermsOfUseController.TermsOfUse)"
                           asp-controller="@typeof(TermsOfUseController).ControllerName()">Terms of Use</a>
                    </li>
                </ul>
                <div>
                    <p class="nhsuk-footer__copyright">© NHS England</p>
                </div>
            </div>

        </div>
    </div>
</footer>
